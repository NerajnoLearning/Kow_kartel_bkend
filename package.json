{
  "name": "kow_kartel_bkend",
  "version": "1.0.0",
  "description": "KitchenOnWheels Rentals Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:headed": "cypress run --headed",
    "test:auth": "cypress run --spec 'cypress/e2e/api/01-auth.cy.ts'",
    "test:equipment": "cypress run --spec 'cypress/e2e/api/02-equipment.cy.ts'",
    "test:bookings": "cypress run --spec 'cypress/e2e/api/03-bookings.cy.ts'",
    "test:payments": "cypress run --spec 'cypress/e2e/api/04-payments.cy.ts'",
    "cypress:verify": "cypress verify",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NerajnoLearning/Kow_kartel_bkend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NerajnoLearning/Kow_kartel_bkend/issues"
  },
  "homepage": "https://github.com/NerajnoLearning/Kow_kartel_bkend#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.917.0",
    "@aws-sdk/s3-request-presigner": "^3.917.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.61.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "socket.io": "^4.8.1",
    "stripe": "^19.1.0",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^23.2.1",
    "@bahmutov/cy-api": "^2.2.8",
    "@cypress/grep": "^5.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "cypress": "^15.5.0",
    "eslint": "^9.38.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
